[autoload]

GameManager="*res://plugins/AbilityFramework/managers/game_manager.tscn"
DebugManager="*res://plugins/AbilityFramework/managers/debug_manager.tscn"
CombatManager="*res://plugins/AbilityFramework/managers/combat_manager.tscn"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
